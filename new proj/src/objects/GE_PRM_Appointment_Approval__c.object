    <validationRules>
        <fullName>GE_PRM_Comments_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND( GE_PRM_I_Understand__c = TRUE,  ISPICKVAL( GE_PRM_Status__c , &apos;Rejected&apos;), ISBLANK(GE_PRM_Comments__c ))</errorConditionFormula>
