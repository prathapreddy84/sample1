    <validationRules>
        <fullName>CMF_Approved_Account_Check_CDCL_FastTra</fullName>
        <active>true</active>
        <description>This evaluates the Account Request Status = CMF Approved to Fast Track or CD CL</description>
        <errorConditionFormula>AND(  
 ISPICKVAL(GE_ES_Opportunity__r.GE_HQ_Business_Tier2__c, &apos;Measurement &amp; Control (M&amp;C)&apos;), 
     NOT(TEXT(GE_ES_Opportunity__r.Account.GE_HQ_Request_Status__c) = &quot;CMF Approved&quot;),
     OR( GE_ES_Opportunity__r.GE_ES_L0L1_Risk_Process_Only__c = True,
         GE_App_Conf_by_CD_CL__c =True )
   )</errorConditionFormula>
        <errorMessage>An Opportunity associated to an Account that is not CMF Approved cannot be Fast Tracked or CD/CL Approved until the Account is CMF Approved. Please Submit the Account to CMF or Check the Status of CMF Request.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CMF_Approved_Acount_Check_for_PGcheckbox</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( GE_ES_PrpslGen__c =true,    
       ISPICKVAL(GE_ES_Opportunity__r.GE_HQ_Business_Tier2__c, &apos;Measurement &amp; Control (M&amp;C)&apos;),     
       
      NOT(TEXT(GE_ES_Opportunity__r.Account.GE_HQ_Request_Status__c) = &quot;CMF Approved&quot;))</errorConditionFormula>
        <errorMessage>An Opportunity must have a CMF Approved Account before submitting Proposal to customer. Please Submit the Account for CMF review or add a CMF Approved Account to this Oppty.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GE_EM_Check_CD_CL</fullName>
        <active>true</active>
        <description>This validation is used to prevent the user to check Approvals confirmed by CD/CL until  Risk Cheklist is complete.</description>
        <errorConditionFormula>AND(
ISPICKVAL(GE_ES_Opportunity__r.GE_HQ_Business_Tier2__c, &apos;Measurement &amp; Control (M&amp;C)&apos;),
ISCHANGED(GE_App_Conf_by_CD_CL__c), GE_App_Conf_by_CD_CL__c = True, OR(GE_EM_Count_CDCL__c &lt;&gt; GE_Count_Checklist__c , AND(GE_Count_Checklist__c=0, 
ISPICKVAL(GE_ES_Opportunity__r.GE_DP_Price_Apr_Stat__c, &apos;Pending&apos; )), 
ISPICKVAL(GE_ES_Opportunity__r.GE_DP_Price_Apr_Stat__c, &apos;Pending&apos; )))</errorConditionFormula>
        <errorDisplayField>GE_ES_Account__c</errorDisplayField>
        <errorMessage>Please submit all risk factors (including pricing if applicable) before proceeding with Verbal approval.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GE_ESMCS_R2CompleteCheck</fullName>
        <active>true</active>
        <description>For MCS only, if the user is trying to change R Status to R2 Complete without Proposal Generated checkbox checked then give error.</description>
        <errorConditionFormula>AND ( ISPICKVAL(GE_ES_Opportunity__r.GE_HQ_Business_Tier1__c,&apos;Oil &amp; Gas (O&amp;G)&apos;),
         ISPICKVAL(GE_ES_Opportunity__r.GE_HQ_Business_Tier2__c,&apos;Measurement &amp; Control (M&amp;C)&apos;), 
         NOT( ISPICKVAL(GE_ES_Opportunity__r.GE_ES_PGS_Sub_P_L__c,&apos;Flow &amp; Process Technologies (Dresser)&apos;)),
         ISCHANGED( GE_OG_GS_RStatus__c ) ,
         ISPICKVAL(GE_OG_GS_RStatus__c, &apos;R2 Complete&apos;), 
         GE_ES_PrpslGen__c =false)</errorConditionFormula>
        <errorMessage>You cannot move to R2 Complete if proposal is not generated yet.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GE_ES_Checking_Proposal_Generated</fullName>
        <active>true</active>
        <description>Proposal Generated Checks on Deal Management</description>
        <errorConditionFormula>AND( 
ISPICKVAL(GE_ES_Opportunity__r.GE_HQ_Business_Tier2__c, &apos;Measurement &amp; Control (M&amp;C)&apos;),
ISCHANGED(GE_OG_GS_Date__c),
GE_ES_PrpslGen__c = true,
GE_App_Conf_by_CD_CL__c = False,
NOT(ISPICKVAL(GE_Checklist_Status__c,&apos;Approved&apos;)),
NOT(OR(ISPICKVAL(GE_ES_Risk_Path__c,&apos;Express&apos;))
)
)</errorConditionFormula>
        <errorMessage>Risk path not completed correctly.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GE_ES_Pre_Bid_Questions_DMO</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
    ISPICKVAL(GE_ES_Opportunity__r.GE_HQ_Business_Tier2__c, &apos;Measurement &amp; Control (M&amp;C)&apos;), 
    OR(
	NOT(ISPICKVAL(GE_ES_Opportunity__r.GE_ES_Standard_Product__c, &apos;Yes&apos;)), 
        NOT(ISPICKVAL(GE_ES_Opportunity__r.GE_ES_Engineering_Approval__c, &apos;No&apos;)),
	NOT(ISPICKVAL(GE_ES_Opportunity__r.GE_ES_On_Site__c,&apos;No&apos;)),
	NOT(ISPICKVAL(GE_ES_Opportunity__r.GE_ES_Multi_Year__c, &apos;No&apos;)),
	NOT(ISPICKVAL(GE_ES_Opportunity__r.GE_ES_Audit_or_Boycott__c,&apos;No&apos;)),
	NOT(ISPICKVAL(GE_ES_Opportunity__r.GE_ES_Export_Credit_or_Free_Trade__c, &apos;No&apos;)),
	NOT(ISPICKVAL(GE_ES_Opportunity__r.GE_ES_Governing_Law_Dispute_Resolution__c,&apos;Yes&apos;)),
	NOT(ISPICKVAL(GE_ES_Opportunity__r.GE_ES_LDs_15_Contract__c, &apos;No&apos;)),
	NOT(ISPICKVAL(GE_ES_Opportunity__r.GE_ES_Deal_Size_Credit_Limit__c, &apos;Yes&apos;)),
	NOT(ISPICKVAL(GE_ES_Opportunity__r.GE_ES_Non_Functional_Currency__c, &apos;No&apos;)),
	NOT(ISPICKVAL(GE_ES_Opportunity__r.GE_ES_New_Tax_Presence__c,&apos;No&apos;)),
	NOT(ISPICKVAL(GE_ES_Opportunity__r.GE_ES_End_User_not_Buyer_or_CE__c, &apos;Yes&apos;)),
	NOT(ISPICKVAL(GE_ES_Opportunity__r.GE_ES_NRE__c,&apos;No&apos;)),
	NOT(ISPICKVAL(GE_ES_Opportunity__r.GE_ES_US_Gov_or_ARRA__c, &apos;No&apos;)),
	NOT(ISPICKVAL(GE_ES_Opportunity__r.GE_ES_Country_Restrictions__c, &apos;No&apos;))
       ),
    GE_ES_Opportunity__r.GE_ES_L0L1_Risk_Process_Only__c =True, 
    OR(AND(NOT(ISNEW()),
           PRIORVALUE(GE_Policy__c)&lt;&gt;GE_Policy__c),
	   ISNEW())
)</errorConditionFormula>
        <errorDisplayField>GE_ES_Account__c</errorDisplayField>
        <errorMessage>This opportunity is not eligible for the P5.0 Fast Track process based on the answers given in Pre Bid Screening Questions</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GE_ES_Regional_Approver_Validation</fullName>
        <active>false</active>
        <description>You cannot add Regional Approver until the Risk Path is Fast Track with Regional Approval.</description>
        <errorConditionFormula>AND( OR(ISPICKVAL(GE_ES_Opportunity__r.GE_HQ_Business_Tier1__c, &quot;Energy Management (EM)&quot;), 
        ISPICKVAL(GE_ES_Opportunity__r.GE_HQ_Business_Tier2__c, &apos;Measurement &amp; Control (M&amp;C)&apos;),
        ISPICKVAL(GE_ES_Opportunity__r.GE_HQ_Business_Tier2__c, &apos;Power Gen Services&apos;) ,
        ISPICKVAL(GE_ES_Opportunity__r.GE_ES_PGS_Sub_P_L__c, &apos;Air Filtration&apos;) ),
    NOT( ISPICKVAL( GE_ES_Risk_Path__c , &apos;Fast Track with Regional Approval&apos;)),
    ISCHANGED(GE_ES_Regnal_App__c) ,
    NOT( Len( GE_ES_Regnal_App__c)=0))</errorConditionFormula>
        <errorMessage>You cannot add Regional Approver until the Risk Path is Fast Track with Regional Approval..</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GE_ES_Required_Fields_Stage_Closed</fullName>
        <active>false</active>
        <description>R2 Complete date, Proposal Stamp Date, Ship-to-region, &amp; Disposition date are required when the stage is changed to closed won or closed partially won.</description>
        <errorConditionFormula>AND((GE_ES_Opportunity__r.RecordTypeId  &lt;&gt; &apos;012A0000000jpRY&apos;),(GE_ES_Opportunity__r.RecordTypeId  &lt;&gt; &apos;012A00000003bUh&apos;),(GE_ES_Opportunity__r.RecordTypeId  &lt;&gt; &apos;012A00000003bUn&apos;),(AND(OR(ISPICKVAL(GE_ES_Opportunity__r.GE_HQ_Business_Tier1__c, &quot;Energy Management (EM)&quot;), ISPICKVAL(GE_ES_Opportunity__r.GE_HQ_Business_Tier2__c, &apos;Measurement &amp; Control (M&amp;C)&apos;), ISPICKVAL(GE_ES_Opportunity__r.GE_HQ_Business_Tier2__c, &apos;Power Gen Services&apos;) ,ISPICKVAL(GE_ES_Opportunity__r.GE_ES_PGS_Sub_P_L__c, &apos;Air Filtration&apos;)),OR(ISPICKVAL(GE_ES_Opportunity__r.StageName, &apos;Closed Won&apos;), ISPICKVAL(GE_ES_Opportunity__r.StageName, &apos;Closed - Partially Won &apos;)), ISBLANK(GE_ES_Opportunity__r.GE_ES_Disposition_Date__c),
ISBLANK(00NA0000009mhc0.00NA0000008c6tW),
ISBLANK(Proposal_Date_Stamp__c),
ISBLANK(GE_ES_R2_Completed_Date__c)))
,  NOT(ISPICKVAL(GE_ES_Opportunity__r.GE_ES_Customer_Type__c, &apos;Channel-Distributor&apos;)))</errorConditionFormula>
        <errorMessage>R2 Complete date, Proposal Stamp Date, Ship-to-region, &amp; Disposition date are required when the stage is changed to closed won or closed partially won.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GE_ES_Update_RStatus_to_R3_Complete</fullName>
        <active>true</active>
        <description>R Status has to be R2 Complete or R3 Queue to move to R3 Complete</description>
        <errorConditionFormula>AND( OR (                ISPICKVAL(GE_ES_Opportunity__r.GE_HQ_Business_Tier2__c,&apos;Measurement &amp; Control (M&amp;C)&apos;)
         ,AND(ISPICKVAL(GE_ES_Opportunity__r.GE_HQ_Business_Tier1__c,&apos;Oil &amp; Gas (O&amp;G)&apos;),
              OR(ISPICKVAL(GE_ES_Opportunity__r.GE_HQ_Business_Tier2__c,&apos;Global Services (GS)&apos;),GE_ES_Opportunity__r.RecordType.Name=&apos;OG TMS Service Opportunity&apos;,GE_ES_Opportunity__r.RecordType.Name=&apos;OG DTS Service Opportunity&apos;))),    
 ISCHANGED(GE_OG_GS_RStatus__c ) ,
 ISPICKVAL(GE_OG_GS_RStatus__c ,&apos;R3 Complete&apos;),
 TEXT(PRIORVALUE( GE_OG_GS_RStatus__c  ))&lt;&gt;&apos;R2 Complete&apos;,
 TEXT(PRIORVALUE( GE_OG_GS_RStatus__c  ))&lt;&gt; &apos;R3 Complete&apos;,
 TEXT(PRIORVALUE( GE_OG_GS_RStatus__c  ))&lt;&gt; &apos;R3 Queue&apos;, 
 $Profile.Name &lt;&gt;&apos;GE_ES System Integration&apos;
)</errorConditionFormula>
        <errorMessage>R Status has to be R2 Complete or R3 Queue to move to R3 Complete</errorMessage>
    </validationRules>
