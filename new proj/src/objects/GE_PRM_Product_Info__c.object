    <validationRules>
        <fullName>Account_Plan_Is_Approved</fullName>
        <active>true</active>
        <description>If Account Plan is &apos;Approved&apos; user shouldn&apos;t be able to create or edit market coverage</description>
        <errorConditionFormula>AND(AND( $Profile.Name &lt;&gt; &apos;GE_ES System Administrator&apos;, $Profile.Name &lt;&gt; &apos;System Administrator&apos;) ,ISPICKVAL( GE_PRM_Account_Plan__r.GE_PRM_Status__c , &apos;Approved&apos;))</errorConditionFormula>
        <errorMessage>You cannot create/edit Market Coverage since Account Plan is Approved</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GE_PRM_Expected_Sales</fullName>
        <active>true</active>
        <errorConditionFormula>AND(GE_PRM_Delete__c = false, ISPICKVAL( GE_PRM_Type__c ,&apos;Expected Sales&apos;), OR(GE_PRM_Product_Service_Line__c = &apos;&apos;, GE_PRM_Product_Service_Line__c = null))</errorConditionFormula>
        <errorDisplayField>GE_PRM_Product_Service_Line__c</errorDisplayField>
        <errorMessage>You must enter Product/Service Line</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GE_PRM_Expected_Sales_Expected_Sales_1</fullName>
        <active>true</active>
        <errorConditionFormula>AND(GE_PRM_Delete__c = false, ISPICKVAL( GE_PRM_Type__c ,&apos;Expected Sales&apos;), OR(GE_PRM_Expected_Sales_Year_1__c == null, ISBLANK(GE_PRM_Expected_Sales_Year_1__c)))</errorConditionFormula>
        <errorDisplayField>GE_PRM_Expected_Sales_Year_1__c</errorDisplayField>
        <errorMessage>You must enter Expected Sales Year 1</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GE_PRM_Expected_Sales_Expected_Sales_2</fullName>
        <active>true</active>
        <errorConditionFormula>AND(GE_PRM_Delete__c = false, ISPICKVAL( GE_PRM_Type__c ,&apos;Expected Sales&apos;), OR(GE_PRM_Expected_Sales_Year_2__c == null, ISBLANK(GE_PRM_Expected_Sales_Year_2__c)))</errorConditionFormula>
        <errorDisplayField>GE_PRM_Expected_Sales_Year_2__c</errorDisplayField>
        <errorMessage>You must enter Expected Sales Year 2</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GE_PRM_Product_Line</fullName>
        <active>true</active>
        <errorConditionFormula>AND(GE_PRM_Delete__c = false, ISPICKVAL( GE_PRM_Type__c ,&apos;Product lines or Services&apos;), OR( GE_PRM_Manufacturer__c = &apos;&apos;, GE_PRM_Manufacturer__c = null))</errorConditionFormula>
        <errorDisplayField>GE_PRM_Manufacturer__c</errorDisplayField>
        <errorMessage>You must enter Manufacturer</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GE_PRM_Product_Line_HeadQuarters_Require</fullName>
        <active>true</active>
        <errorConditionFormula>AND(GE_PRM_Delete__c = false, ISPICKVAL( GE_PRM_Type__c ,&apos;Product lines or Services&apos;), ISBLANK(GE_PRM_Name_of_Country_of_Headquarters__c))</errorConditionFormula>
        <errorDisplayField>GE_PRM_Name_of_Country_of_Headquarters__c</errorDisplayField>
        <errorMessage>You must enter Manufacturer&apos;s Headquarters</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GE_PRM_Product_Line_Prior_Year_Sales</fullName>
        <active>true</active>
        <errorConditionFormula>AND(GE_PRM_Delete__c = false, ISPICKVAL( GE_PRM_Type__c ,&apos;Product lines or Services&apos;), OR(GE_PRM_Prior_Year_Sales_Turnover_USD__c == null, ISBLANK(GE_PRM_Prior_Year_Sales_Turnover_USD__c )))</errorConditionFormula>
        <errorDisplayField>GE_PRM_Prior_Year_Sales_Turnover_USD__c</errorDisplayField>
        <errorMessage>You must enter Prior Year Sales/Turnover</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GE_PRM_Product_Line_Product_Services</fullName>
        <active>true</active>
        <errorConditionFormula>AND(GE_PRM_Delete__c = false, ISPICKVAL( GE_PRM_Type__c ,&apos;Product lines or Services&apos;), OR( GE_PRM_Products_Services__c = &apos;&apos;, GE_PRM_Products_Services__c = null))</errorConditionFormula>
        <errorDisplayField>GE_PRM_Products_Services__c</errorDisplayField>
        <errorMessage>You must enter Products/Services</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GE_PRM_Product_Line_Tenure</fullName>
