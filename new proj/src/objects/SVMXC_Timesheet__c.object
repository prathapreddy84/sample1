    <fields>
        <fullName>FST_Lead_ID__c</fullName>
        <description>Returns the user ID of the technician&apos;s manager.  Used to control visibility of the Timesheet Review toolbar</description>
        <externalId>false</externalId>
        <formula>IF( isblank( Technician__r.SVMXC__Salesforce_User__c ),&quot;NA&quot;,  Technician__r.SVMXC__Salesforce_User__r.Manager.Id)</formula>
        <label>FST Lead ID</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>GE_DS_Download_to_Mobile__c</fullName>
        <externalId>false</externalId>
        <formula>AND(
  OR( 
    AND( YEAR( Start_Date__c ) = YEAR( TODAY() ), MONTH( Start_Date__c ) &lt;= (MONTH( TODAY() ) + 1) )
    ,
    AND( YEAR( Start_Date__c ) = (YEAR( TODAY() ) - 1), MONTH( Start_Date__c ) &gt;= MONTH( TODAY() ) )
  )
  ,
  AND(
  NOT(ISBLANK($Setup.GE_DS_Tech_Assignment__c.GE_DS_Technician_ID__c)),
  $Setup.GE_DS_Tech_Assignment__c.GE_DS_Technician_ID__c = Technician__r.Id)
)</formula>
        <label>Download to Mobile?</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
