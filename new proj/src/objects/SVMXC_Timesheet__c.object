    <validationRules>
        <fullName>GE_DS_Lock_Submitted_Timesheet</fullName>
        <active>false</active>
        <description>If a Timesheet has been submitted, it is locked to the Technician.</description>
        <errorConditionFormula>($Profile.Name &lt;&gt; &apos;GE_DS_Titanium Dispatcher&apos; &amp;&amp; $Profile.Name &lt;&gt; &apos;GE_DS_Titanium Operations Support&apos; &amp;&amp; $Profile.Name &lt;&gt; &apos;GE_DS_Titanium Service Manager&apos; &amp;&amp; $Profile.Name &lt;&gt; &apos;GE_ES Developer&apos;) &amp;&amp;
(
(TEXT(PRIORVALUE(GE_OG_DS_Status__c)) = &quot;Submitted&quot; ||
TEXT(PRIORVALUE(GE_OG_DS_Status__c)) = &quot;Approved&quot;) &amp;&amp;
((TEXT(GE_OG_DS_Status__c) != &quot;New&quot;) || (TEXT(GE_OG_DS_Status__c) != &quot;&quot;))
)</errorConditionFormula>
        <errorMessage>A Timesheet cannot be edited after it has been submitted.</errorMessage>
    </validationRules>
