    <fields>
        <fullName>Allow_Admin_Functions__c</fullName>
        <description>Formula that determines if a user can see the review timesheet toolbar</description>
        <externalId>false</externalId>
        <formula>if(contains( FST_Lead_ID__c ,$User.Id),1,0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Allow Admin Functions</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Download_to_iPad__c</fullName>
        <externalId>false</externalId>
        <formula>If (CONTAINS(  Related_User__c  , $User.Id ) &amp;&amp; ( End_Date__c &gt;= TODAY()-15), 1, 0)</formula>
        <label>Download to iPad</label>
        <precision>18</precision>
