    <validationRules>
        <fullName>GE_HQ_Eff_Dt_Cannot_Be_Set_In_Past</fullName>
        <active>false</active>
        <description>Rule to enforce the requirement that effective dates on Region Matrix records cannot be set in the past. They must be today or future.</description>
        <errorConditionFormula>GE_HQ_Eff_Dt__c  &lt;=Today()</errorConditionFormula>
        <errorDisplayField>GE_HQ_Eff_Dt__c</errorDisplayField>
        <errorMessage>Effective Date cannot be set to the past</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GE_HQ_ExpDt_is_greater_than_EffDt</fullName>
        <active>true</active>
        <errorConditionFormula>GE_HQ_Exp_Dt__c  &lt;=  GE_HQ_Eff_Dt__c</errorConditionFormula>
        <errorDisplayField>GE_HQ_Eff_Dt__c</errorDisplayField>
        <errorMessage>Effective Date must be less than or equal to Expiration Date.</errorMessage>
