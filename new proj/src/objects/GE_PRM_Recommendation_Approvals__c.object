    <validationRules>
        <fullName>Check_I_Understand_To_Reject</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(GE_PRM_Status__c, &apos;Rejected&apos;)  , GE_PRM_I_Understand__c  = false)</errorConditionFormula>
        <errorDisplayField>GE_PRM_I_Understand__c</errorDisplayField>
        <errorMessage>I understand that if this recommendation is rejected, it will not be reversible.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Comments_Required</fullName>
        <active>true</active>
        <description>Comments are Required for Reject and Soft Reject</description>
        <errorConditionFormula>AND(
OR(
ISPICKVAL(GE_PRM_Status__c, &apos;Rejected&apos;),
ISPICKVAL(GE_PRM_Status__c, &apos;Need More Info&apos;)),
 GE_PRM_Comments__c ==&apos;&apos;)</errorConditionFormula>
        <errorDisplayField>GE_PRM_Comments__c</errorDisplayField>
        <errorMessage>Please Provide Comments</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GE_PRM_Cannot_Select_Rejection_Flag</fullName>
        <active>true</active>
        <errorConditionFormula>AND( OR(ISPICKVAL( GE_PRM_Status__c , &apos;Approved&apos;), ISPICKVAL( GE_PRM_Status__c , &apos;Need More Info&apos;), ISPICKVAL( GE_PRM_Status__c , &apos;Pending&apos;) ),  GE_PRM_I_Understand__c   )</errorConditionFormula>
        <errorDisplayField>GE_PRM_I_Understand__c</errorDisplayField>
        <errorMessage>Please uncheck Rejection flag</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GE_PRM_Finance_Section_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISPICKVAL(GE_PRM_Recommendation__r.GE_PRM_Agreement_Category__c, &apos;Termination&apos;)), NOT(ISPICKVAL(GE_PRM_Recommendation__r.GE_PRM_Agreement_Category__c, &apos;Non-Renewal&apos;)), GE_PRM_Role__c = &apos;Region Commercial Finance&apos;, ISCHANGED( GE_PRM_Status__c), ISPICKVAL( GE_PRM_Status__c , &apos;Approved&apos;), OR( ISPICKVAL(GE_PRM_Recommendation__r.GE_PRM_Required_Payment_Terms__c, &apos;&apos;), ISPICKVAL( GE_PRM_Recommendation__r.GE_PRM_Non_standard_payment_terms__c , &apos;&apos;), ISPICKVAL( GE_PRM_Recommendation__r.GE_PRM_Needs_credit_limit__c , &apos;&apos;) ))</errorConditionFormula>
        <errorMessage>The following fields on the Recommendation must be completed before approval: Required payment terms, Non-standard payment terms, and Needs credit limit</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Not_Authorized_to_Take_Approval_Actions</fullName>
        <active>false</active>
        <description>Not_Authorized_to_Take_Approval_Actions</description>
        <errorConditionFormula>AND(
OR(
ISPICKVAL(GE_PRM_Status__c, &apos;Approved&apos;),
ISPICKVAL(GE_PRM_Status__c, &apos;Rejected&apos;),
ISPICKVAL(GE_PRM_Status__c, &apos;Need More Info&apos;)),
AND( $User.GE_HQ_SSO_ID__c &lt;&gt;  GE_PRM_Approvers__r.GE_HQ_SSO_ID__c , $User.GE_HQ_SSO_ID__c &lt;&gt;  GE_PRM_Delegated_User__r.GE_HQ_SSO_ID__c))</errorConditionFormula>
        <errorMessage>You are NOT Authorized to take Approval Actions</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Recommendation_Locked</fullName>
        <active>true</active>
        <description>No Actions can be taken if the Recommendation is Approved or Rejected</description>
        <errorConditionFormula>OR(
ISPICKVAL( GE_PRM_Recommendation__r.GE_PRM_Status__c, &apos;Rejected&apos;),
ISPICKVAL( GE_PRM_Recommendation__r.GE_PRM_Status__c, &apos;Approved&apos;))</errorConditionFormula>
        <errorMessage>Recommendation is locked, You cannot make any more changes.</errorMessage>
    </validationRules>
</CustomObject>
