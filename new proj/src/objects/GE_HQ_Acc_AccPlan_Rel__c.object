    <webLinks>
        <fullName>GE_HQ_Del</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Delete</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>var RelIds = {!GETRECORDIDS($ObjectType.GE_HQ_Acc_AccPlan_Rel__c)};
if(RelIds.length ==0 || RelIds ==&apos;&apos;)
alert(&apos;Please select at least one Account to delete&apos;);
else if(confirm(&apos;Are you sure, you want to delete the selected Accounts&apos;))
window.location.href=&apos;/apex/GE_HQ_MyAccountController?Action=Del&amp;RelIds=&apos;+RelIds;</url>
    </webLinks>
