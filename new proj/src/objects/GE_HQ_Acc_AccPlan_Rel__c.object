    <webLinks>
        <fullName>Add_to_Account_Team</fullName>
        <availability>online</availability>
        <description>Select Multiple MY GE Account and can add to the Account Team</description>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Add to Account Team</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>var MyGEAccIds = {!GETRECORDIDS($ObjectType.GE_HQ_Acc_AccPlan_Rel__c)};
if(MyGEAccIds.length ==0 || MyGEAccIds ==&apos;&apos;)
alert(&apos;Please select at least one Account, where you want to become a team member.&apos;);
else
window.location.href=&apos;/apex/GE_OG_MyGEAcc_Add?Action=AT&amp;MyGEAccIds=&apos;+MyGEAccIds;</url>
    </webLinks>
