    <webLinks>
        <fullName>GE_HQ_GoogleMap</fullName>
        <availability>online</availability>
        <description>Plot the selected accounts on a Google Map.</description>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Google Map!</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>var myGEAccountIds = {!GETRECORDIDS($ObjectType.GE_HQ_Acc_AccPlan_Rel__c)};
if(myGEAccountIds.length ==0 || myGEAccountIds ==&apos;&apos;)
alert(&apos;Please select at least one Account.&apos;);
else if(myGEAccountIds.length&gt;40)
alert(&apos;You have selected more than 40 Accounts, please select less or equal to 40 Accounts&apos;);
else{
window.location.href=&apos;/apex/GE_HQ_GoogleMap?AccIds=&apos;+myGEAccountIds;
}</url>
    </webLinks>
    <webLinks>
        <fullName>GE_OG_RemoveFromAccTeam</fullName>
        <availability>online</availability>
        <description>user will be remove from the Ownership of the team</description>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Remove from AccountTeam</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>var MyGEAccIds = {!GETRECORDIDS($ObjectType.GE_HQ_Acc_AccPlan_Rel__c)};
if(MyGEAccIds.length ==0 || MyGEAccIds ==&apos;&apos;)
alert(&apos;Please select at least one Account, where you no longer wish to be a team member&apos;);
else {
var ans = confirm(&apos; You will no longer be a member of the selected accounts. Are you Sure?&apos;);
if(ans) 
window.location.href=&apos;/apex/GE_OG_MyGEAcc_Add?Action=RT&amp;MyGEAccIds=&apos;+MyGEAccIds;
}</url>
    </webLinks>
